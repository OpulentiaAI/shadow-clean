{
  "version": 2,
  "installCommand": "npm ci",
  "buildCommand": "npx turbo build --filter=server",
  "framework": null,
  "functions": {
    "api/backend.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/backend"
    }
  ]
}
